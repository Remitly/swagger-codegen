package service

{{#operations}}
import (
    "github.com/remitly/tiny-middleware-go/opentracing"
    "net/http"
){{#operation}}

func {{nickname}}(w http.ResponseWriter, r *http.Request) {
    opentracing.StartSpanFromRequest(r, "{{nickname}}")
